---
import Layout from "@layout/Layout.astro";
import ArrowRight from "@atoms/ArrowRight.astro";

const aiAgentsStructuredData = {
    "@context": "https://schema.org",
    "@type": "WebPage",
    name: "AI-agents | Brix Studio",
    description:
        "Automatiseer content, SEO en communicatie processen met AI, zonder je merkidentiteit te verliezen met AI-agents die jouw bedrijf écht begrijpen.",
    url: "https://www.brixstudio.nl/ai-agents",
    isPartOf: {
        "@type": "WebSite",
        name: "Brix Studio",
        url: "https://www.brixstudio.nl",
    },
    mainEntity: {
        "@type": "Service",
        name: "AI-agents",
        description:
            "Automatiseer content, SEO en communicatie processen met AI, zonder je merkidentiteit te verliezen met AI-agents die jouw bedrijf écht begrijpen.",
        provider: {
            "@type": "Organization",
            name: "Brix Studio",
            url: "https://www.brixstudio.nl",
        },
        offers: {
            "@type": "Offer",
            price: "0",
            priceCurrency: "EUR",
            availability: "https://schema.org/InStock",
            url: "https://www.brixstudio.nl/contact",
        },
    },
};
---

<Layout
    title="AI-agents | Brix Studio"
    description="Automatiseer content, SEO en communicatie processen met AI, zonder je merkidentiteit te verliezen met AI-agents die jouw bedrijf écht begrijpen."
    structuredData={aiAgentsStructuredData}
>
    <section class="ai-agents-hero">
        <div class="hero-content">
            <h1>AI-agents</h1>
            <h2>Jouw bedrijf op autopiloot met AI.</h2>
            <p class="hero-subtitle">
                Automatiseer content, SEO en communicatie processen met AI,
                zonder je merkidentiteit te verliezen met AI-agents die jouw
                bedrijf écht begrijpen.
            </p>
            <a href="/contact" class="cta-button"
                tabindex="0">Gratis adviesgesprek <ArrowRight /></a
            >
        </div>
        <small
            >of <a href="#how-it-works"
                tabindex="0">scroll naar beneden om meer te ontdekken</a
            ></small
        >
    </section>

    <section class="how-it-works" id="how-it-works">
        <div class="section-container">
            <h2>Hoe het werkt</h2>
            <p class="section-intro">
                Onze tools sluiten aan op je bestaande systemen en processen.
                Geen gedoe. Geen leercurve. Gewoon een slimme uitbreiding van je
                team die 24/7 klaarstaat.
            </p>

            <div class="steps">
                <div class="step">
                    <div class="step-number">1</div>
                    <div class="step-content">
                        <h3>Intake & analyse</h3>
                        <p>
                            Gratis eerste gesprek en analyse van je huidige
                            content en bedrijfsprocessen.
                        </p>
                    </div>
                </div>
                <div class="step">
                    <div class="step-number">2</div>
                    <div class="step-content">
                        <h3>Maatwerk AI-workflow</h3>
                        <p>
                            We ontwikkelen een op maat gemaakte AI-workflow
                            specifiek voor jouw behoeften.
                        </p>
                    </div>
                </div>
                <div class="step">
                    <div class="step-number">3</div>
                    <div class="step-content">
                        <h3>Test & optimalisatie</h3>
                        <p>
                            We testen de AI-agent grondig en optimaliseren deze
                            op basis van feedback.
                        </p>
                    </div>
                </div>
                <div class="step">
                    <div class="step-number">4</div>
                    <div class="step-content">
                        <h3>Live inzet & groei</h3>
                        <p>
                            De AI-agent gaat live en blijft zich ontwikkelen
                            naarmate je bedrijf groeit.
                        </p>
                    </div>
                </div>
            </div>
        </div>
